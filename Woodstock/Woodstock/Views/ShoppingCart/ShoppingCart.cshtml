@model IEnumerable<ShoppingCartViewModel>

@{
    ViewBag.Title = "Shopping Cart";
}

<table class="table_price">
    <caption>Sale</caption>
    <tr>
        <th>
            Фото
        </th>
        <th>
            Название
        </th>
        <th>
            Описание
        </th>
        <th>
            Цена
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Watch.Photo
            </td>
            <td>
                @item.Watch.Title
            </td>
            <td>
                @item.Watch.Description
            </td>
            <td>
                @item.Watch.Price
            </td>
        </tr>
    }
</table>